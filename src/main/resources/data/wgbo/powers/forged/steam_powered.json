  "consume_resource_delay": {
    "type": "origins:resource",
    "max": 8,
    "min": 0,
    "hud_render": {
      "should_render": false
    }
  },
  "hold_buffer": {
    "type": "origins:resource",
    "min": 0,
    "max": 3,
    "hud_render": {
      "should_render": false
    }
  },
  "clean_hold_buffer": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_hold_buffer",
      "change": -1
    },
    "interval": 1,
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_hold_buffer",
      "comparison": ">",
      "compare_to": 0
    }
  },
  "stop_consumption": {
    "type": "origins:action_over_time",
    "interval": 1,
    "falling_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:change_resource",
          "resource": "*:*_consume_resource",
          "operation": "set",
          "change": 0
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_consume_resource_delay",
          "operation": "set",
          "change": 0
        }
      ]
    },
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_hold_buffer",
      "comparison": ">",
      "compare_to": 1
    }
  }
