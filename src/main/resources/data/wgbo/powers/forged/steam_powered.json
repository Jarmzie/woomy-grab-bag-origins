{
  "type": "origins:multiple",
  "inefficient_engineering": {
    "type": "origins:modify_exhaustion",
    "modifier": {
      "name": "Increased exhaustion",
      "operation": "multiply_base",
      "value": 2.0
    }
  },
  "synthetic_biology": {
    "type": "origins:prevent_item_use",
    "item_condition": {
      "type": "origins:food"
    }
  },
  "refuel": {
    "type": "origins:active_self",
    "cooldown": 1,
    "key": {
      "key": "key.use",
      "continuous": true
    },
    "hud_render": {
      "should_render": false
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:equipped_item",
          "equipment_slot": "mainhand",
          "item_condition": {
            "type": "origins:ingredient",
            "ingredient": {
              "tag": "wgbo:forged_fuel"
            }
          }
        },
        {
          "type": "origins:food_level",
          "comparison": "<",
          "compare_to": 20
        }
      ]
    },
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:resource",
            "resource": "*:*_consume_resource_delay",
            "comparison": "==",
            "compare_to": 8
          },
          "if_action": {
            "type": "origins:change_resource",
            "resource": "*:*_consume_resource",
            "operation": "add",
            "change": 1
          }
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_consume_resource_delay",
          "operation": "add",
          "change": 1
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_hold_buffer",
          "change": 2
        }
      ]
    }
  },

  "fake_eat": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "execute at @s positioned ~ ~1.5 ~ run particle minecraft:flame ^ ^ ^0.3 .09 .03 .03 0 3",
          "permission_level": 4
        },
        {
          "type": "origins:execute_command",
          "command": "playsound minecraft:item.firecharge.use player @a[distance=..10] ~ ~ ~ 1 1",
          "permission_level": 4
        }
      ]
    },
    "interval": 4,
    "condition": {
      "type": "origins:and",
      "inverted": false,
      "conditions": [
        {
          "type": "origins:resource",
          "inverted": false,
          "resource": "*:*_consume_resource",
          "comparison": "!=",
          "compare_to": 0
        },
        {
          "type": "origins:resource",
          "inverted": false,
          "resource": "*:*_consume_resource",
          "comparison": "<",
          "compare_to": 32
        }
      ]
    }
  },

  "fake_eat_slow_movement": {
    "type": "origins:conditioned_attribute",
    "modifier": {
      "attribute": "minecraft:generic.movement_speed",
      "operation": "multiply_total",
      "value": -0.8
    },
    "condition": {
      "type": "origins:resource",
      "inverted": false,
      "resource": "*:*_consume_resource_delay",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "fake_eat_slow_air_speed": {
