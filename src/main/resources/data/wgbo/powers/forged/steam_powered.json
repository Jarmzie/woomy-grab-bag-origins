          },
          "if_action": {
            "type": "origins:feed",
            "food": 4,
            "saturation": 2.4
          }
        },
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:equipped_item",
            "equipment_slot": "mainhand",
            "item_condition": {
              "type": "origins:ingredient",
              "ingredient": {
                "item": "minecraft:coal"
              }
            }
          },
          "if_action": {
            "type": "origins:feed",
